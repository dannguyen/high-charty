{
  "chart": {
    "attributes": [
      {
        "name": "width",
        "type": "integer"
      },
      {
        "name": "height",
        "type": "integer",
        "defaultValue": 400
      },
      {
        "name": "type",
        "values": [
          "area",
          "bar",
          "column",
          "line",
          "pie",
          "scatter"
        ],
        "defaultValue": "bar"
      },
      {
        "name": "stacking",
        "values": [
          "stacked",
          "grouped"
        ],
        "defaultValue": "grouped"
      }
    ]
  },
  "data": {
    "attributes": [
      {
        "name": "text",
        "type": "text"
      },
      {
        "name": "delimiter",
        "defaultValue": ","
      },
      {
        "name": "xKey"
      },
      {
        "name": "yKey"
      },
      {
        "name": "seriesKey"
      },
      {
        "name": "categoryKey"
      },
      {
        "name": "seriesColors"
      }
    ]
  },
  "axis": {
    "attributes": [
      {
        "name": "dimension",
        "values": [
          "x",
          "y",
          "z"
        ]
      },
      {
        "name": "title"
      },
      {
        "name": "tickInterval",
        "type": "integer"
      },
      {
        "name": "categories",
        "type": "boolean"
      }
    ]
  }
}
