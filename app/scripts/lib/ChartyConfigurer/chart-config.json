{
  "chart": {
    "attributes": [
      {
        "name": "width",
        "type": "integer",
        "defaultValue": "100%"
      },
      {
        "name": "height",
        "type": "integer",
        "defaultValue": 400
      },
      {
        "name": "type",
        "values": [
          "area",
          "bar",
          "column",
          "line",
          "pie",
          "scatter"
        ],
        "defaultValue": "bar"
      },
      {
        "name": "stacking",
        "values": [
          "stacked",
          "grouped"
        ],
        "defaultValue": "grouped"
      }
    ]
  },
  "data": {
    "attributes": [
      {
        "name": "text",
        "type": "text"
      },
      {
        "name": "delimiter",
        "defaultValue": ","
      },
      {
        "name": "seriesColors"
      }
    ]
  },
  "axis": {
    "attributes": [
      {
        "name": "title"
      },
      {
        "name": "tickPixelInterval",
        "type": "integer"
      },
      {
        "name": "categorical",
        "type": "boolean"
      }
    ]
  }
}
